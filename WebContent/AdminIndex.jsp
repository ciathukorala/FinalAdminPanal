<!-- comments -->
<script type='text/javascript'>
$(document).ready(function()
{
$.ajax
({
type: "GET",
url: "GetMessages1",
dataType:"json",
success: function(data)
{
if(data.Messages.length)
{
$.each(data.Messages, function(i,data)
{
var msg_data1="<div>"+data.Name+"</div>";
var msg_data2="<div>"+data.Email+"</div>";
var msg_data3="<div>"+data.Subject+"</div>";
var msg_data4="<div>"+data.Message+"</div>";
$(msg_data1).appendTo("#content1");
$(msg_data2).appendTo("#content2");
$(msg_data3).appendTo("#content3");
$(msg_data4).appendTo("#content4");

});
}

}
});

});

</script>

